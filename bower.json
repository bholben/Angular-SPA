{
  "name": "Gulp-Template",
  "version": "0.0.1",
  "description": "Web app template with gulpfile that compiles/injects Jade, Stylus, Bower refs",
  "homepage": "https://github.com/bholben/gulp-template",
  "license": "MIT",
  "authors": [
    "Bob Holben <bholben@gmail.com>"
  ],
  "dependencies": {
    "angular": "~1.3.2",
    "angular-route": "~1.3.3",
    "angular-animate": "~1.3.3",
    "angular-ui-router": "~0.2.13",
    "font-awesome": "~4.2.0"
  },
  "overrides": {
    "angular": {
      "main": {
        "dev": [
          "./angular.js"
        ],
        "prod": [
          "./angular.min.js"
        ]
      }
    },
    "angular-animate": {
      "main": {
        "dev": [
          "./angular-animate.js"
        ],
        "prod": [
          "./angular-animate.min.js"
        ]
      }
    },
    "angular-route": {
      "main": {
        "dev": [
          "./angular-route.js"
        ],
        "prod": [
          "./angular-route.min.js"
        ]
      }
    },
    "font-awesome": {
      "main": {
        "dev": [
          "./css/font-awesome.css",
          "./fonts/*"
        ],
        "prod": [
          "./css/font-awesome.min.css",
          "./fonts/*"
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bholben/gulp-template.git"
  }
}
