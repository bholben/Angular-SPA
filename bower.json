{
  "name": "Angular-SPA",
  "version": "0.0.1",
  "description": "Angular Single-page App boilerplate",
  "homepage": "https://github.com/bholben/Angular-SPA",
  "license": "MIT",
  "authors": [
    "Bob Holben <bholben@gmail.com>"
  ],
  "dependencies": {
    "angular": "~1.3.2",
    "angular-animate": "~1.3.3",
    "angular-ui-router": "~0.2.13",
    "bootstrap": "~3.3.1",
    "font-awesome": "~4.2.0"
  },
  "overrides": {
    "angular": {
      "main": {
        "dev": [
          "./angular.js"
        ],
        "prod": [
          "./angular.min.js"
        ]
      }
    },
    "angular-animate": {
      "main": {
        "dev": [
          "./angular-animate.js"
        ],
        "prod": [
          "./angular-animate.min.js"
        ]
      }
    },
    "angular-ui-router": {
      "main": {
        "dev": [
          "./release/angular-ui-router.js"
        ],
        "prod": [
          "./release/angular-ui-router.min.js"
        ]
      }
    },
    "bootstrap": {
      "main": {
        "dev": [
          "dist/css/bootstrap.css",
          "dist/css/bootstrap.css.map",
          "dist/fonts/glyphicons-halflings-regular.eot",
          "dist/fonts/glyphicons-halflings-regular.svg",
          "dist/fonts/glyphicons-halflings-regular.ttf",
          "dist/fonts/glyphicons-halflings-regular.woff"
        ],
        "prod": [
          "dist/css/bootstrap.min.css",
          "dist/css/bootstrap.css.map",
          "dist/fonts/glyphicons-halflings-regular.eot",
          "dist/fonts/glyphicons-halflings-regular.svg",
          "dist/fonts/glyphicons-halflings-regular.ttf",
          "dist/fonts/glyphicons-halflings-regular.woff"
        ]
      }
    },
    "font-awesome": {
      "main": {
        "dev": [
          "./css/font-awesome.css",
          "./fonts/*"
        ],
        "prod": [
          "./css/font-awesome.min.css",
          "./fonts/*"
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bholben/gulp-template.git"
  }
}
